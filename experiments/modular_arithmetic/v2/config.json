{
  "version": "v2_conflict_split",
  "split_type": "structured_sum_based",
  "split_rule": "(a+b)%p < p/2 (train) vs >= p/2 (test)",
  "sanity_check": "Passed at 20k steps (see v2b_conflict_split/)",
  "data": {
    "modulus_p": 97,
    "train_split": 0.8,
    "seed": 42
  },
  "model": {
    "n_layers": 4,
    "n_heads": 4,
    "d_model": 256,
    "d_ff": 1024,
    "dropout": 0.1,
    "max_seq_len": 16,
    "vocab_size": 104
  },
  "training": {
    "learning_rate": 0.001,
    "weight_decay": 0.0,
    "batch_size": 256,
    "num_steps": 400000,
    "log_interval": 100,
    "checkpoint_interval": 5000,
    "eval_num_batches": null
  },
  "analysis": {
    "compute_rank_interval": 500,
    "compute_entropy_interval": 500,
    "rank_threshold": 0.01,
    "analysis_num_batches": 10
  }
}